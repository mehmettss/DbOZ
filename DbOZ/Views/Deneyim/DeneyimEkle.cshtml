
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Yeni Deneyim Ekleme Sayfası</h4>
<br />
<form method="post" class="form-group">
   
    <input type="text" name="Baslık" class="form-control" placeholder="Baslıgı Yazınız..."/>
    <br />
    <input type="text" name="AltBaslik" class="form-control" placeholder="AltBaslık Yazınız..."/>
    <br />
    <input type="text" name="Tarih" class="form-control" placeholder="Calısma Tarihi..."/>
    <br />
    <textarea placeholder="Acıklama Yazınız..." name="Aciklama" class="form-control" rows="5"></textarea>
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>

