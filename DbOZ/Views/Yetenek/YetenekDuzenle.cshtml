
@model DbOZ.Models.Entity.Tbl_yeteneklerim

@{
    ViewBag.Title = "YetenekDuzenle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<br />
<h4>Yetenek Duzenleme Sayfası</h4>
<br />
<form method="post">
    @Html.Label("Yetenek Acıklama")
    @Html.TextBoxFor(x => x.Yetenek, new { @class = "form-control" })
    <br />
    @Html.Label("Derece")
    @Html.TextBoxFor(x => x.Oran, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">Guncelle</button>
</form>

